/* pages/yueku/yueku.wxss */
@import '../font/iconFonts.wxss';

page {
  background: url('http://r.photo.store.qq.com/psb?/V11cXZik1xvWSk/eGmXLj5oGkdBCmdDsCxSeo*nIYSTQmozxnvPALYjp.8!/r/dPMAAAAAAAAA');
  height: 85%;
  font-family: "宋体";
}
navigator{
  width: 100%;
  height: 100%;
}
.title {
  margin-top: 30rpx;
  color: #555;
  font-size: 30rpx;
  text-align: left;
  /* border: 1px solid red; */
   padding-left: 15px; 
  padding-bottom: 10rpx;
}
.title text{
  margin-left: 10rpx;
}
.title image {
  width: 40rpx;
  height: 40rpx;
}

.list {
  position: relative;
  width: 100%;
  height: 180rpx;
  margin-top: 15rpx;
    display: flex;   
   padding-left: 15px; 
  /* border: 1px solid red; */
  /* background-color: #eee; */
   flex-wrap: nowrap; 
}

.list image {
  width: 150rpx;
  height: 150rpx;
}

.list .right{
  /* border: 1px solid red; */
  width: 72%;
  height:75px;
  background-color: #eee;
  box-shadow: 1px 4px 3px #999;
  border-radius: 3px;
}
.content {
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;

  margin-right: 20rpx;
  position: absolute;
  top: 15%;
  left: 25%;
   color: #666;
}

.name {
  position: absolute;
  right: 15%;
  bottom: 35%;
  font-size: 30rpx;
   color: #444;

}

.list .collect {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  left: 25%;
  bottom: 36%;
  
}

.collectContent {
  position: absolute;
  bottom: 35%;
  left: 30%;
  font-size: 30rpx;
  color: #555;
}
